<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffe5e5"
    tools:context="com.example.sudoajay.memorygame.MainLogoDesgin">

    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:textColor="#153972"
        android:textSize="25sp"
        app:layout_constraintRight_toLeftOf="@+id/restartButton"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.269"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.034" />

    <Button
        android:id="@+id/restartButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Restart"
        android:textColor="#153972"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.034" />

    <Button
        android:id="@+id/menuButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Menu"
        android:textColor="#153972"
        android:textSize="25sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/restartButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.034" />

    <Button
        android:id="@+id/stepsButton"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="monospace"
        android:text="Steps- "
        android:textColor="#153972"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.941" />

    <Button
        android:id="@+id/hintButton"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="monospace"
        android:text="Hint"
        android:textColor="#153972"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.941" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintRight_toLeftOf="@+id/imageButton8"
        app:layout_constraintLeft_toRightOf="@+id/imageButton6"
        app:layout_constraintBottom_toTopOf="@+id/imageButton12"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton11"
        app:layout_constraintRight_toLeftOf="@+id/imageButton13"
         app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton17"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7" />

    <ImageButton
        android:id="@+id/imageButton17"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton16"
        app:layout_constraintRight_toLeftOf="@+id/imageButton18"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/stepsButton"
        app:layout_constraintTop_toBottomOf="@+id/imageButton12" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton1"
        app:layout_constraintRight_toLeftOf="@+id/imageButton3"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintTop_toBottomOf="@+id/backButton"
        app:layout_constraintBottom_toTopOf="@+id/imageButton7" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton7"
        app:layout_constraintRight_toLeftOf="@+id/imageButton9"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton13"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton12"
        app:layout_constraintRight_toLeftOf="@+id/imageButton14"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton18"
        app:layout_constraintTop_toBottomOf="@+id/imageButton8" />

    <ImageButton
        android:id="@+id/imageButton18"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton17"
        app:layout_constraintRight_toLeftOf="@+id/imageButton19"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.784" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton2"
        app:layout_constraintRight_toLeftOf="@+id/imageButton4"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton8"
        app:layout_constraintTop_toBottomOf="@+id/restartButton" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton8"
        app:layout_constraintRight_toLeftOf="@+id/imageButton10"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton14"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton13"
        app:layout_constraintRight_toLeftOf="@+id/imageButton15"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton19"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9" />

    <ImageButton
        android:id="@+id/imageButton19"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton18"
        app:layout_constraintRight_toLeftOf="@+id/imageButton20"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/hintButton"
        app:layout_constraintTop_toBottomOf="@+id/imageButton14" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton3"
        app:layout_constraintRight_toLeftOf="@+id/imageButton5"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintTop_toBottomOf="@+id/restartButton"
        app:layout_constraintBottom_toTopOf="@+id/imageButton9" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton9"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton15"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5" />

    <ImageButton
        android:id="@+id/imageButton15"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton14"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton20"
        app:layout_constraintTop_toBottomOf="@+id/imageButton10" />

    <ImageButton
        android:id="@+id/imageButton20"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton19"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/hintButton"
        app:layout_constraintTop_toBottomOf="@+id/imageButton15" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton4"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintTop_toBottomOf="@+id/menuButton"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintRight_toLeftOf="@+id/imageButton7"
        app:layout_constraintBottom_toTopOf="@+id/imageButton11"
        app:layout_constraintTop_toBottomOf="@+id/imageButton1" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/imageButton12"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/imageButton16"
        app:layout_constraintTop_toBottomOf="@+id/imageButton6" />

    <ImageButton
        android:id="@+id/imageButton16"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/imageButton17"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintBottom_toTopOf="@+id/stepsButton"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="60dp"
        android:layout_height="70dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/imageButton2"
        app:srcCompat="@drawable/hiderimage"
        app:layout_constraintTop_toBottomOf="@+id/backButton"
        app:layout_constraintBottom_toTopOf="@+id/imageButton6" />

</android.support.constraint.ConstraintLayout>
